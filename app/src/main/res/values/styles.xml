<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="BottomNavigation">
        <item name="android:background">@color/colorPrimaryDark</item>
        <item name="itemIconTint">@color/bnvColorIcon</item>
        <item name="itemTextColor">@color/bnvColorTextIcon</item>
    </style>


    <style name="MyStyleButton">
        <item name="colorButtonNormal">@android:color/holo_orange_dark</item>
        <item name="colorControlHighlight">@android:color/holo_orange_dark</item>
        <item name="android:textColor">@android:color/black</item>
    </style>

</resources>
